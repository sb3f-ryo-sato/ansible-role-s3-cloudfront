{
    "Version": "2008-10-17",
    "Id": "PolicyForCloudFrontPrivateContent",
    "Statement": [
	{
	    "Sid": "1",
	    "Effect": "Allow",
	    "Principal": {
		"AWS": "arn:aws:iam::cloudfront:user/CloudFront Origin Access Identity {{ item.s3_origin_config.origin_access_identity | regex_replace('origin-access-identity/cloudfront/') }}"
	    },
	    "Action": "s3:GetObject",
	    "Resource": "arn:aws:s3:::{{ s3_bucket_name }}/*"
	}
    ]
}
